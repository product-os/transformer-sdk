import type { Contract, ContractDefinition } from '<%= sdk.name %>'
<% for (const type of types) { %>
<%- type.data %>
export interface <%= type.contractInterfaceName %> extends Contract<<%= type.dataInterfaceName %> > {
    type: '<%= type.handle %>'
}

export interface <%= type.contractDefinitionInterfaceName %> extends ContractDefinition<<%= type.dataInterfaceName %> > {
type: '<%= type.handle %>'
}
<% } %>
